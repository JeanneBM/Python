print(mojalista)
['papuga', 9, 'mysz', 'kot', 1, 2.4, 'papuga', 10]

nowalista = mojalista[0:3]

print(mojalista)
['papuga', 9, 'mysz', 'kot', 1, 2.4, 'papuga', 10]

print(nowalista)
['papuga', 9, 'mysz']

-----------------------------------------------------------------------------

print(mojalista)
['papuga', 9, 'mysz', 'kot', 1, 2.4, 'papuga', 10]

nowalista = mojalista[2:5]

print(nowalista)
['mysz', 'kot', 1]

-----------------------------------------------------------------------------

print(mojalista)
['papuga', 9, 'mysz', 'kot', 1, 2.4, 'papuga', 10]

nowalista = mojalista[3:-2]

print(nowalista)
['kot', 1, 2.4]

-----------------------------------------------------------------------------

print(mojalista)
['papuga', 9, 'mysz', 'kot', 1, 2.4, 'papuga', 10]

nowalista = mojalista[1:6:2]

print(nowalista)
[9, 'kot', 2.4]

-----------------------------------------------------------------------------


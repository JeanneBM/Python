print(mojalista)
['papuga', 9, 'mysz', 'kot', 1, 2.4, 'papuga', 10]

nowalista = mojalista[0:3]

print(mojalista)
['papuga', 9, 'mysz', 'kot', 1, 2.4, 'papuga', 10]

print(nowalista)
['papuga', 9, 'mysz']

-----------------------------------------------------------------------------

print(mojalista)
['papuga', 9, 'mysz', 'kot', 1, 2.4, 'papuga', 10]

nowalista = mojalista[2:5]

print(nowalista)
['mysz', 'kot', 1]

-----------------------------------------------------------------------------


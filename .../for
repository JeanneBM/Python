for i in range (1,5):
  print ("*"*i)
  
-----------------------------------------------------------------------------  
  
s = 0
for x in range(1,3):
    for y in range(1,4):   
        s = s + 2
print (s)

-----------------------------------------------------------------------------  

s = 2
for x in range (1,2):
   for y in range (5,7):
      s = s + x + y
print (s)

----------------------------------------------------------------------------- 

s = 0
for x in range (2,4):
   x = 0
   for y in range (-1,1):
      x = x + y
      s = s + x
print (s)

-----------------------------------------------------------------------------

s = 0
for x in [2,6,4]:
   for y in range (9,11):
      s = s + x + y
print (s)


i=1

while i <5:
  print ("*"*i)
  i+= 1
else: 
  print ("|_|")
  
-----------------------------------------------------------------------------  

s = -20
x = 2
while x < 5:
    y = 2
    while y < 5:
        s = s + x + y
        y = y + 1
    x = x + 1
print(s)


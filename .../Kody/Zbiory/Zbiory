zbior = {'kot', 'pies', 'mysz', 'kot', 'chomik', 'kaczka'}
print(zbior)

{'mysz', 'kaczka', 'kot', 'pies', 'chomik'}

---------------------------------------------------------------------------------

lista1 = ['a', 2, 'b', 3, 'c', 1, 'a']
print(lista1)
zbior1 = set(lista1)
print(zbior1)
lista2 = list(zbior1)
print(lista2)

['a', 2, 'b', 3, 'c', 1, 'a']
{1, 2, 3, 'b', 'a', 'c'}
[1, 2, 3, 'b', 'a', 'c']

---------------------------------------------------------------------------------

zbior1 = {1,2,3}
zbior1.add(4)
print(zbior1)

{1, 2, 3, 4}

---------------------------------------------------------------------------------

zbior1 = {1,2,3}
zbior1.remove(2)
print(zbior1)

{1, 3}

---------------------------------------------------------------------------------

zbior1 = {1,2,3}
print(3 in zbior1)

True

---------------------------------------------------------------------------------

zbior1 = {1,2,3,4,5,4,3,2,1} #5 - powtorzone liczby
print(len(zbior1))

5 

---------------------------------------------------------------------------------

zbior1 = {1,2,3,4,5,4,3,'kot'}
print(zbior1)

{1, 2, 3, 4, 5, 'kot'}

---------------------------------------------------------------------------------

zbior1 = {1,2,3,4,5,4,3,'kot'}
print(type(zbior1))

<class 'set'>

---------------------------------------------------------------------------------

zbior1 = {1,2,3,4,5,4,3,'kot'}
zbior1.remove(3)
print(len(zbior1))

5

---------------------------------------------------------------------------------

zbior1 = {0, 1, 2, 3}
print('1' in zbior1)



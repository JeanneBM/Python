from unittest import TestCase
class SimpleTest(TestCase):
    def not_equal(self):
        self.assertNotEqual(2, 3)  # 2 != 3
    def assert_false(self):
        x = 0
        self.assertFalse(x)   # bool(x) jest równe False
    def assert_in(self):
        self.assertIn(5, [1, 3, 8, 5])    # 5 zawiera się w [1, 3, 8, 5]
